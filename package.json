{
    "name": "react-youtube",
    "version": "1.0.0",
    "description": "react-youtube",
    "main": "index.js",
    "engines": {
        "node": "12.16.1",
        "npm": "7.20.5"
    },
    "scripts": {
        "start": "node server/index.js",
        "backend": "node server/index.js",
        "frontend": "npm run start --prefix client",
        "setup": "node --stack-size=1600000 server/index.js",
