        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "client": "^0.0.1",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.5",
        "debug": "^4.1.1",
        "express": "^4.17.1",
        "ffprobe": "^1.1.2",
        "fluent-ffmpeg": "^2.1.2",
        "http-proxy-middleware": "^2.0.1",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "mongoose": "^5.4.20",
        "multer": "^1.4.2",
        "react-redux": "^5.1.2",
        "react-scripts": "3.4.1",
        "redux": "^4.1.1",
        "redux-thunk": "^2.3.0",
        "saslprep": "^1.0.3",
